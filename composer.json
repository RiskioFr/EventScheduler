{
    "name": "riskio/schedule-module",
    "description": "Manage events including recurring ones",
    "homepage": "https://github.com/RiskioFr/ScheduleModule",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.riskio.fr"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "zendframework/zend-eventmanager": "~2.4",
        "zendframework/zend-servicemanager": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\ScheduleModule\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\ScheduleModuleTest\\": "tests/"
        }
    }
}
