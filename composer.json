{
    "name": "riskio/event-scheduler",
    "description": "Manage recurring events",
    "homepage": "https://github.com/RiskioFr/EventScheduler",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.riskio.fr"
        }
    ],
    "require": {
        "php": ">=5.6",
        "nicolopignatelli/valueobjects": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\EventScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\EventSchedulerTest\\": "tests/"
        }
    }
}
