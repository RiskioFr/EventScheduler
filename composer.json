{
    "name": "riskio/schedule",
    "description": "Manage events including recurring ones",
    "homepage": "https://github.com/RiskioFr/Schedule",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.riskio.fr"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\Schedule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\ScheduleTest\\": "tests/"
        }
    }
}
